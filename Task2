using System;
class Task2
{
    public void Main()
    {
        double h, m;

        Console.WriteLine("Введите рост(в метрах): ");
        h = Convert.ToDouble(Console.ReadLine());
        Console.WriteLine("Введите массу тела(в киллограмах): ");
        m = Convert.ToDouble(Console.ReadLine());

        Console.WriteLine("ИТМ равен: " + m / (h * h));
        Console.ReadKey();
        Console.Clear();
    }

}
